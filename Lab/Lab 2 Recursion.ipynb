   "source": [
    "# a function calls itself -> termination condition to prevent errors\n",
    "\n",
    "def fact(n):\n",
    "    if n==1:\n",
    "        return 1\n",
    "    f = n * fact(n-1)\n",
    "    return f\n",
    "\n",
    "fact(6)"
   ]
